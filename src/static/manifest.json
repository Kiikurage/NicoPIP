{
    "manifest_version": 2,
    "name": "NicoPIP",
    "description": "",
    "version": "0.0.1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ],
    "web_accessible_resources": ["*"],
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": ["storage"]
}
